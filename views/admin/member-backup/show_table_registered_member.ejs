          <div  class="tab-pane fade" id="show-registered-member" role="tabpanel" aria-labelledby="registered-tab">
            <!-- DataTales Example -->
            <div class="card shadow mb-4 mt-2">
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                      <tr>
                        <th>No all</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>User Name</th>
                        <th>Email</th>
                        <th>Reset Password</th>
                        <th>Action</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(let i = 0; i< member.length; i++){ %>
                        <!-- read data catagory -->
                            <tr>
                              <td><%= i + 1 %></td>
                              <td><%= member[i].firstName %></td>
                              <td><%= member[i].lastName %></td>
                              <td><%= member[i].userName %></td>
                              <td><%= member[i].email %></td>
                              <td>
                                <form action="/admin/member/<%= member[i].id%>/delete?_method=DELETE" method="POST">
                                  <!--  cara mengambil id dan name dari data -->
                                  <button type="submit" class='btn btn-danger btn-circle btn-sm'><i class = "fas fa-trash"></i></button>
                                  <!-- <a href="/admin/member/show-detail-item/<%= member[i].id%>" class= 'btn btn-success btn-circle btn-sm'><i class = "fas fa-plus"></i></a> -->
                                </form>
                                </td>
                              <td>
                                <form action="/admin/member/<%= member[i].id%>/delete?_method=DELETE" method="POST">
                                  <!--  cara mengambil id dan name dari data -->
                                  <a href="/admin/member/<%= member[i].id%>" class='btn btn-warning btn-circle btn-sm'><i class = "fas fa-edit"></i></a>  <!-- javascript void(): untuk menghandle action button-->
                                  <button type="submit" class='btn btn-danger btn-circle btn-sm'><i class = "fas fa-trash"></i></button>
                                  <!-- <a href="/admin/member/show-detail-item/<%= member[i].id%>" class= 'btn btn-success btn-circle btn-sm'><i class = "fas fa-plus"></i></a> -->
                                </form>
                                </td>
                            </tr>
                     <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>