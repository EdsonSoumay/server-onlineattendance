          <div class="tab-pane fade show active" id="show-all-member" role="tabpanel" aria-labelledby="home-tab">
            <!-- DataTales Example -->
            <div class="card shadow mb-4 mt-2">
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                      <tr>
                        <th>No</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>User Name</th>
                        <th>Email</th>
                        <th>Status</th>
                        <th>Role</th>
                        <th>Change Role</th>
                        <th>Reset Password</th>
                        <th>Action</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(let i = 0; i< member.length; i++){ %>
                        <!-- read data catagory -->
                            <tr>
                              <td><%= i + 1 %></td>
                              <td><%= member[i].firstName %></td>
                              <td><%= member[i].lastName %></td>
                              <td><%= member[i].userName %></td>
                              <td><%= member[i].email %></td>
                              <td>
                                <% if(member[i].isActive == false){ %>
                                  <span style="color: #ccba2e;">
                                    Inactive
                                  </span>
                                  <% } %>
                                <% if(member[i].isActive == true){  %>
                                  <span style="color: #cc0000;">
                                    Active
                                  </span>
                                  <% } %>
                              </td>
                              <td><%= member[i].role %></td>
                            <td>
                              <form action="/admin/member/role/<%= member[i].id%>?_method=PUT" method="POST">
                                <div class="modal-body">
                                  <select class="form-control"  id="role" name="role" >
                                    <option value="President">President</option>
                                    <option value="Vice President">Vice President</option>
                                    <option value="Treasure">Treasure</option>
                                    <option value="Secretary">Secretary</option>
                                    <option value="Member">Member</option>
                                </select>
                                </div>
                                <button type="submit" class="btn btn-primary">ok</button>
                            </form>
                            </td>
                           
                            <td>
                              <form action="/admin/member/password/<%= member[i].id%>?_method=PUT" method="POST">
                                <!--  cara mengambil id dan name dari data -->
                                <button type="submit" class='btn btn-primary btn-sm'>Reset Password</button>
                              </form>
                            </td>
                              
                              <td>
                              <!-- form Delete -->
                                <form action="/admin/member/<%= member[i]._id%>?_method=DELETE" method="POST">
                                  <!--  cara mengambil id dan name dari data -->
                                  <button type="submit" class='btn btn-danger btn-sm'>Hapus</button>
                                  <!-- <a href="/admin/member/show-detail-item/<%= member[i].id%>" class= 'btn btn-success btn-circle btn-sm'><i class = "fas fa-plus"></i></a> -->
                                </form>
                                <br/>
                                <!-- form Update Status -->
                                <form action="/admin/member/status/<%= member[i].id%>?_method=PUT" method="POST">
                                  <!--  cara mengambil id dan name dari data -->
                                  <button type="submit" class='btn btn-primary btn-sm'>Update Status</button>
                                </form>
                                <br/>                             
                                </td>
                            </tr>
                     <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>