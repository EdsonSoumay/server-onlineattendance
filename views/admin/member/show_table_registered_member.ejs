          <div  class="tab-pane fade" id="show-registered-member" role="tabpanel" aria-labelledby="registered-tab">
            <!-- DataTales Example -->
            <div class="card shadow mb-4 mt-2">
              <div class="card-body">
                <div class="table-responsive">
                  <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                      <tr>
                        <th>No all</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>User Name</th>
                        <th>Email</th>
                        <th>Accept</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(let i = 0; i< registeredMember.length; i++){ %>
                        <!-- read data catagory -->
                            <tr>
                              <td><%= i + 1 %></td>
                              <td><%= registeredMember[i].firstName %></td>
                              <td><%= registeredMember[i].lastName %></td>
                              <td><%= registeredMember[i].userName %></td>
                              <td><%= registeredMember[i].email %></td>
                              <td>
                                <form action="/admin/registeredmember/<%= registeredMember[i].id%>/<%= registeredMember[i].userName %>/<%= registeredMember[i].email %>?_method=PUT"  method="POST">
                                  <!--  cara mengambil id dan name dari data -->
                                  <button type="submit" class='btn btn-primary btn-sm'>Yes</button>
                                  <!-- <a href="/admin/member/show-detail-item/<%= member[i].id%>" class= 'btn btn-success btn-circle btn-sm'><i class = "fas fa-plus"></i></a> -->
                                </form>
                                <form action="/admin/member/<%= registeredMember[i]._id%>?_method=DELETE" method="POST">
                                  <!--  cara mengambil id dan name dari data -->
                                  <button type="submit" class='btn btn-danger btn-sm'>No</button>
                                  <!-- <a href="/admin/member/show-detail-item/<%= registeredMember[i].id%>" class= 'btn btn-success btn-circle btn-sm'><i class = "fas fa-plus"></i></a> -->
                                </form>
                                </td>
                            </tr>
                     <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>